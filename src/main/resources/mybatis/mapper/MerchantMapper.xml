<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.dblab2.mapper.MerchantMapper">
    <insert id="addMerchant" parameterType="Merchant">
        insert into merchant(m_name) values(#{m_name})
    </insert>

    <delete id="deleteMerchant" parameterType="Merchant">
        delete from merchant where m_name=#{m_name}
    </delete>

    <select id="selectMerchant" resultType="Merchant">
        select * from merchant;
    </select>
</mapper>